#!/bin/python
import subprocess
import socket

def run(command):
   output = subprocess.check_output(command, shell=True).strip()
   output = output.decode('utf-8')
   return output

try:
    ssid = run("iwgetid -r")
    wifi = "яки"
    print(wifi + " "  + ssid)
except:
    print("якй Disconnected")
